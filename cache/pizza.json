        },
		always : function (e, data) {
			$("#progressbar_block_aside").hide();
			$("#imgsearchbutton_block_aside").show();
			$("#imgsearchmsg_block_aside").hide();
        },
		start: function (e, data) {
			$("#imgsearchbutton_block_aside").hide();
			$("#imgsearcherror_block_aside").hide();
			$("#imgsearchmsg_block_aside").show();
			$("#progressbar_block_aside").show();
			$("#progressmeter_block_aside").css('width', "0%");
                    
		},
            sent: function (e, data) {
                if (data.dataType &&
                        data.dataType.substr(0, 6) === 'iframe') {
                    // Iframe Transport does not support progress events.
                    // In lack of an indeterminate progress bar, we set
                    // the progress to 100%, showing the full animated bar:
                    $("#progressmeter_block_aside").css('width', "100%");
                }
            },
            progress: function (e, data) {

                   $("#progressmeter_block_aside").css('width', parseInt(data.loaded / data.total * 100, 10) + "%");
					$("#imgsearchdebug_block_aside").html(data.loaded + ' / ' + data.total);
                
            }
		
    });	
	

});
